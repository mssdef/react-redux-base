    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "music-player",
    "semantic-ui",
    "e-commerce",
    "magento"
  ],
  "author": "Music Player Team",
  "license": "MIT"
}